{
  "name": "kuaukutsu/ds-collection",
  "description": "Is the abstract layer which covers functionality the data structure Collection.",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ds-collection/issues",
    "source": "https://github.com/kuaukutsu/ds-collection"
  },
  "require": {
    "php": "^7.4|^8.0",
    "php-ds/php-ds": "^1.3",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "roave/infection-static-analysis-plugin": "^1.3",
    "rector/rector": "^0.14.7",
    "vimeo/psalm": "^4.1",
    "psalm/plugin-phpunit": "^0.13.0",
    "squizlabs/php_codesniffer": "*",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpunit/phpunit": "^9.5",
    "thecodingmachine/safe": "v1.3.3"
  },
  "scripts": {
    "post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
    "post-update-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ds\\collection\\": "src",
      "kuaukutsu\\ds\\collection\\tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.4.32"
    },
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
