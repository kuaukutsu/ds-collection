{
  "name": "kuaukutsu/ds-collection",
  "description": "Is the abstract layer which covers functionality the data structure Collection.",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ds-collection/issues",
    "source": "https://github.com/kuaukutsu/ds-collection"
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "php-ds/php-ds": "^1.3"
  },
  "require-dev": {
    "vimeo/psalm": "^5.15",
    "friendsofphp/php-cs-fixer": "^3.13",
    "icanhazstring/composer-unused": "^0.8",
    "phpunit/phpunit": "^10.4",
    "rector/rector": "^1.2",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.7",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ds\\collection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\ds\\collection\\tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
